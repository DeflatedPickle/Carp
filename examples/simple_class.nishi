package Carp

class MyClass {
    private {
        my_int: 1337
        my_empty -> Integer
    }

    public {
        name: "MyClass"
    }

    fun MyClass() {
        start: "Hello"
        end: "World!"

        start: "Bye"

        print(start, ", ", end)
    }

    fun hello() -> Void {
        print("Hello, World!")
    }

    fun hello(friend -> String) -> Void {
        print("Hello, ", friend)
    }
}